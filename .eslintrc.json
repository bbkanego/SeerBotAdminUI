{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliendStrict": true
        }
    },
    "env": {
        "browser": true,
        "node": false
    },
    "rules": {
        "no-var": "off",
        "linebreak-style": "off",
        "max-len": [2, {"code": 100, "tabWidth": 4, "ignoreUrls": true}]
    }

}